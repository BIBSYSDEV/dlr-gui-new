{
  "name": "dlr-gui-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build:development": "env-cmd -f ./.env.development npm run-script build",
    "build:production": "env-cmd -f ./.env.production npm run-script build",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "start:development": "env-cmd -f ./.env.development npm run-script start",
    "start:production": "env-cmd -f ./.env.production npm run-script start",
    "test": "cypress run",
    "test:cypress": "REACT_APP_USE_MOCK=true start-server-and-test http://localhost:3000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/barlow": "^4.5.1",
    "@fontsource/crimson-text": "^4.5.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/lab": "^5.0.0-alpha.68",
    "@mui/material": "^5.4.0",
    "@mui/styled-engine-sc": "^5.3.0",
    "@mui/styles": "^5.3.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@uppy/aws-s3-multipart": "^1.8.18",
    "@uppy/core": "^1.20.1",
    "@uppy/dashboard": "^1.20.2",
    "@uppy/react": "^1.12.2",
    "axios": "^0.26.0",
    "date-fns": "^2.28.0",
    "deepmerge": "^4.2.2",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "i18next": "^21.6.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.4",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-share": "^4.4.0",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@types/history": "^5.0.0",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-truncate": "^2.3.4",
    "@types/styled-components": "^5.1.22",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.13",
    "axios-mock-adapter": "^1.20.0",
    "cypress": "^9.4.1",
    "cypress-file-upload": "^5.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "react-scripts": "^5.0.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.5"
  }
}
